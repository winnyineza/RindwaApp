  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:setup": "./scripts/setup-database.sh",
    "db:reset": "npm run db:setup",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "backup:db": "node scripts/backup-database.js",
    "backup:files": "node scripts/backup-files.js",
    "backup:full": "npm run backup:db && npm run backup:files",
    "health": "curl -f http://localhost:3000/health || exit 1",
    "mobile:android": "cd RindwaMobile && npm run android",
    "mobile:ios": "cd RindwaMobile && npm run ios",
    "mobile:start": "cd RindwaMobile && npm start"
  },
